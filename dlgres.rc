   LTEXT           "R8",            IDS_R8,       PI_FX,                 PI_Y+Y_OFS+130,         12,  10
   LTEXT           " ",             IDC_R8,       PI_FX+15,              PI_Y+130,          REGS_DX,  10, SS_SUNKEN
