   LTEXT           "R7",            IDS_R7,       PI_FX,                 PI_Y+Y_OFS+115,         12,  10
   LTEXT           " ",             IDC_R7,       PI_FX+15,              PI_Y+115,          REGS_DX,  10, SS_SUNKEN
   PUSHBUTTON      "View",          IDB_VIEW_R7,  PI_FX+15+REGS_DX+3,    PI_Y+115,               19,  10
   PUSHBUTTON      "Clear",         IDB_CLEAR_R7, PI_FX+15+REGS_DX+3+21, PI_Y+115,               20,  10
