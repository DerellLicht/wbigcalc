   DEFPUSHBUTTON   "Clear",         IDB_CLEAR_R0, PI_FX+15+REGS_DX+3+21, PI_Y+10,                20,  10
   PUSHBUTTON      "View",          IDB_VIEW_R0,  PI_FX+15+REGS_DX+3,    PI_Y+10,                19,  10
